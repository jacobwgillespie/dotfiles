{
  "files.associations": {
    "Brewfile": "ruby",
    ".gitconfig.*": "properties",
    "keybindings.json.symlink": "jsonc",
    "settings.json.symlink": "jsonc",
    "*.conf.symlink": "properties",
    "*.js.symlink": "javascript",
    "*.json.symlink": "json",
    "*.plist.symlink": "xml",
    "*.symlink": "shellscript",
    "*.yml.symlink": "yaml"
  },
  "editor.formatOnSave": true,
  "cSpell.words": ["brewfile", "dotfiles", "gitignore", "symlink", "symlinked", "symlinks", "tweetbot"],
  "prettier.prettierPath": "node_modules/prettier/index.js"
}
